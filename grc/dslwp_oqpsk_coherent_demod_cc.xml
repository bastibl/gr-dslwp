<block>
  <name>OQPSK Coherent Demod CC</name>
  <key>dslwp_oqpsk_coherent_demod_cc</key>
  <category>[DSLWP]</category>
  <import>import dslwp</import>
  <make>dslwp.oqpsk_coherent_demod_cc($samples_per_symbol, $taps, $opt_point, $pll, $pll_loop_bw, $pll_damping, $freq_max, $freq_min, $dttl, $dttl_loop_bw, $dttl_damping, $max_rate_deviation, $asm_ignore)</make>
  <param>
    <name>Samples per Symbol</name>
    <key>samples_per_symbol</key>
    <value>16</value>
    <type>int</type>
  </param>
  <param>
    <name>Taps</name>
    <key>taps</key>
    <type>complex_vector</type>
  </param>
  <param>
    <name>Opt. Point</name>
    <key>opt_point</key>
    <value>12</value>
    <type>int</type>
  </param>
  <param>
    <name>PLL</name>
    <key>pll</key>
    <value>1</value>
	<type>enum</type>
	<option>
		<name>ON</name>
		<key>1</key>
	</option>
	<option>
		<name>OFF</name>
		<key>0</key>
	</option>
  </param>
  <param>
    <name>PLL Loop BW</name>
    <key>pll_loop_bw</key>
    <value>0.05</value>
    <type>float</type>
  </param>
  <param>
    <name>PLL Damping</name>
    <key>pll_damping</key>
    <value>0.707</value>
    <type>float</type>
  </param>
  <param>
    <name>Freq. Max.</name>
    <key>freq_max</key>
    <value>3.14</value>
    <type>float</type>
  </param>
  <param>
    <name>Freq. Min.</name>
    <key>freq_min</key>
    <value>-3.14</value>
    <type>float</type>
  </param>
  <param>
    <name>DTTL</name>
    <key>dttl</key>
    <value>0</value>
	<type>enum</type>
	<option>
		<name>ON</name>
		<key>1</key>
	</option>
	<option>
		<name>OFF</name>
		<key>0</key>
	</option>
  </param>
  <param>
    <name>DTLL Loop BW</name>
    <key>dttl_loop_bw</key>
    <value>0.05</value>
    <type>float</type>
  </param>
  <param>
    <name>DTTL Damping</name>
    <key>dttl_damping</key>
    <value>0.707</value>
    <type>float</type>
  </param>
  <param>
    <name>Max. Rate Deviation</name>
    <key>max_rate_deviation</key>
    <value>1.5</value>
    <type>float</type>
  </param>
  <param>
    <name>ASM ignore (symbols)</name>
    <key>asm_ignore</key>
    <value>0</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <source>
    <name>eye_diagram</name>
    <type>complex</type>
    <vlen>$samples_per_symbol</vlen>
  </source>
</block>
